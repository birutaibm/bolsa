{
  "name": "bolsa",
  "description": "An API project to a personal finance manager",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/birutaibm/bolsa.git"
  },
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --trace-warnings --ignore-watch node_modules src/main/index.ts",
    "start": "node dist/main/index.js",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000",
    "pretest": "docker-compose up -d postgres",
    "posttest": "docker-compose down"
  },
  "dependencies": {
    "apollo-server-express": "^2.25.3",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.0",
    "dotenv": "^8.6.0",
    "express": "^4.18.0",
    "graphql": "^15.6.1",
    "graphql-type-json": "^0.3.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-typescript": "^7.13.0",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.1",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/pg": "^7.14.11",
    "@types/supertest": "^2.0.10",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "faker": "^5.5.3",
    "jest": "^27.0.4",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.2",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.2"
  }
}
